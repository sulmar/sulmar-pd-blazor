 <div class="nav-item px-3">
            <NavLink class="nav-link" href="@Url"  Match="@Match">
                <span class="bi bi-list-nested-nav-menu" aria-hidden="true"></span> @Title
            </NavLink>
        </div>
@code {
    
    [Parameter]
    public string Url { get; set; }

    [Parameter]
    public string Title { get; set; }

    [Parameter]
    public NavLinkMatch Match { get; set; }

}